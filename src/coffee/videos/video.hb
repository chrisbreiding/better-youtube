<a class="play-video pull-left" href="#">
  <img src="{{thumb}}" width="150" />
  <i class="icon-play-circle"></i>
</a>
<h4>{{title}}</h4>
<p class="pub-date pull-right">{{date published}}</p>
<p class="duration">
  <span class="label label-inverse">
    <i class="icon-time"></i>
    {{duration duration}}
  </span>
  <a href="#" class="mark-watched">Mark Watched</a>
  <a href="#" class="mark-unwatched">Mark Unwatched</a>
</p>
